<%= form_with url: '/' do |form| %>
  <%= form.label :date %>
  <%= form.date_field :date, value: Date.today, min: '2023-01-01', max: '2023-12-31' %>
  <%= form.date_select :date %>
  <br>
  <%= form.label :started %>
  <%# also datetime_local_field%>
  <%= form.datetime_field :started %>
  <%= form.time_field :time %>
  <br>
  <%= form.submit %>
<% end %>
